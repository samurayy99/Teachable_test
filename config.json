{
    "llm_config": {
        "timeout": 600,
        "cache_seed": 42,
        "temperature": 0.5,
        "models": {
            "gpt-4": {
                "api_type": "openai",
                "presence_penalty": 0.5,
                "frequency_penalty": 0.5
            },
            "gpt-3.5-turbo": {
                "api_type": "openai",
                "top_p": 0.9
            }
        }
    },
    "teach_config": {
        "memory": {
            "expiry_duration": "7d"
        },
        "learning": {
            "enable_learning": true,
            "learning_rate": 0.1
        },
        "interaction": {
            "max_response_time": 5,
            "personalization": true
        },
        "security": {
            "data_encryption": true,
            "compliance": ["GDPR", "HIPAA"]
        },
        "api_limits": {
            "requests_per_minute": 10,
            "quota_per_day": 1000
        },
        "logging": {
            "log_level": "INFO",
            "monitor_performance": true
        },
        "modules": {
            "sentiment_analysis": {
                "enabled": true,
                "sensitivity": 0.8
            }
        },
        "feedback": {
            "collect_user_feedback": true,
            "feedback_evaluation_period": "30d"
        }
    }
}
